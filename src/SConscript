Import("env")

librt = env.StaticLibrary(
    target="rt",
    source=[
        "barrier.c",
        "cond.c",
        "list.c",
        "mutex.c",
        "notify.c",
        "once.c",
        "queue.c",
        "rt.c",
        "rwlock.c",
        "sem.c",
        "sleep.c",
    ],
)

Return("librt")
